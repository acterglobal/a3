{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter (debug mode)",
            "cwd": "app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "--dart-define",
                "RAGESHAKE_URL=http://localhost/api/submit"
            ]
        },
        {
            "name": "flutter (release mode)",
            "cwd": "app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define",
                "RAGESHAKE_URL=http://localhost/api/submit"
            ]
        },
        {
            "name": "flutter (profile mode)",
            "cwd": "app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define",
                "RAGESHAKE_URL=http://localhost/api/submit"
            ]
        },
        {
            "name": "rust",
            "type": "lldb",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "cargo": {
                "args": [
                    "test",
                    "-p",
                    "acter"
                ]
            }
        }
    ]
}